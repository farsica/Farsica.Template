{
  "Connection": {
    "SensitiveDataLoggingEnabled": true,
    "DetailedErrorsEnabled": true
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Warning",
        "Microsoft.EntityFrameworkCore.Database.Command": "Warning",
        "Hangfire": "Information"
      }
    }
  }
}